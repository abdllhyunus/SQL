3-)
UPDATE employee
SET 	name = 'Abdullah Yunus',
	    birthday = '1995-06-20',
		email = 'abdullah@gmail.com'
WHERE id = 3
RETURNING *;

UPDATE employee
SET		email = 'gizela@sicely.com'
WHERE id = 28
RETURNING *;

UPDATE employee
SET		birthday = '1997-05-05'
WHERE id = 46
RETURNING *;

UPDATE employee
SET		email = 'palomalikes@me.com'
WHERE id = 45
RETURNING *;

UPDATE employee
SET		name = 'Marcel Station'
WHERE id = 11
RETURNING *;

SELECT * FROM employee
ORDER BY id;

4-)
DELETE FROM employee
WHERE name LIKE 'Maxy%'
RETURNING *;

DELETE FROM employee
WHERE id BETWEEN 23 AND 27
RETURNING *;

DELETE FROM employee
WHERE name ILIKE 'a%h%'
RETURNING *; 

DELETE FROM employee
WHERE email IS NULL
RETURNING *;

DELETE FROM employee
WHERE birthday < '1980-01-01'
RETURNING *;

SELECT * FROM employee
ORDER BY id;
